set autocalc
set numeric
set numeric_decimal=2
set overlap

color "type=HEADINGS fg=WHITE bg=BLACK"
color "type=HEADINGS_ODD fg=WHITE bg=BLACK"
color "type=EXPRESSION fg=GREEN bg=DEFAULT_COLOR"
color "type=STRG fg=WHITE bg=DEFAULT_COLOR"
color "type=CELL_NEGATIVE fg=RED bg=DEFAULT_COLOR"
color "type=CELL_SELECTION fg=BLACK bg=WHITE"
color "type=CELL_SELECTION_SC fg=BLACK bg=WHITE"

nmap "," ":"
nmap "B" ":cellcolor \"bold=1\"<cr>"
vmap "B" ":cellcolor \"bold=1\"<cr>"
